<%= form_for(@user) do |f| %>

  <div>
  	<%= f.label :title %>
  	<%= f.select :title, ["Mr.","Mrs.","Ms.","Dr."]%>
  </div>
  <div>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </div>
  <div>
    <%= f.label :last_name%>
    <%= f.text_field :last_name %>
  </div>


  <div><%= f.submit "Update" %></div>
<% end %>
